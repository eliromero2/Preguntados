<main class="container mt-5">
    <section class="row">
        <article class="col-12">
            <h1>Usuarios registrados</h1>
            <table class="table table-striped">
                <thead>
                <tr>
                    <th>Usuario</th>
                    <th>Nombre</th>
                    <th>Año de nacimiento</th>
                    <th>Email</th>
                    <th>Contraseña</th>
                    <th>Direccion</th>
                    <th>Rol</th>

                </tr>
                </thead>
                <tbody>
                {{#usuarios}}
                    <tr>
                        <td>{{user_name}}</td>
                        <td>{{nombre_completo}}</td>
                        <td>{{ano_nacimiento}}</td>
                        <td>{{mail}}</td>
                        <td>{{password}}</td>
                        <td>{{direccion}}</td>
                        <td>{{rol}}</td>

                    </tr>
                {{/usuarios}}
                </tbody>
            </table>
        </article>
    </section>
</main>